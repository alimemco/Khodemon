<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".Views.fragments.FragmentAddLevelThree">

<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <com.ali.rnp.khodemon.MyLibrary.MyTextView
        android:id="@+id/textInputLayout1234567"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:gravity="center"
        android:text="ابعاد مکان"
        android:textSize="14sp"
        app:fontCustom="shp_iranSansMobileBold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


    </com.ali.rnp.khodemon.MyLibrary.MyTextView>
    <androidx.cardview.widget.CardView
        android:id="@+id/add_level_one_cardView_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        app:cardCornerRadius="8dp"
        app:cardElevation="2dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textInputLayout1234567">

        <com.ali.rnp.khodemon.MyLibrary.MyEditText
            android:id="@+id/fragment_add_level_three_demens_EditText"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white"
            android:drawableRight="@drawable/ic_maracas"
            android:hint="0  متر مربع"
            android:gravity="center"
            android:maxLines="1"
            android:ellipsize="end"
            android:inputType="number"
            android:maxLength="8"
            android:paddingLeft="@dimen/standard_margin"
            android:paddingTop="12dp"
            android:paddingRight="@dimen/standard_margin"
            android:paddingBottom="10dp"
            android:textSize="14sp"
            app:fontCustom="shp_iranSansMobile"

            />
    </androidx.cardview.widget.CardView>
    <com.ali.rnp.khodemon.MyLibrary.MyTextView
        android:id="@+id/fragment_add_level_three_dimenType_TextView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        android:gravity="left"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/add_level_one_cardView_name" />


    <com.ali.rnp.khodemon.MyLibrary.MyTextView
        android:id="@+id/myTextView32"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:gravity="center"
        android:text="ساعت کاری"
        android:textSize="14sp"
        app:fontCustom="shp_iranSansMobileBold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fragment_add_level_three_dimenType_TextView" />
    <androidx.cardview.widget.CardView
        android:id="@+id/fragment_add_level_three_cardView_chooseHours"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        app:cardCornerRadius="8dp"
        app:cardElevation="2dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/myTextView32">

        <com.ali.rnp.khodemon.MyLibrary.MyTextView
            android:id="@+id/fragment_add_level_three_MyTextView_chooseHours"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:drawableLeft="@drawable/ic_arrow_down"
            android:drawableRight="@drawable/ic_saxaphone"
            android:gravity="center"
            android:hint="انتخاب کنید"
            android:maxLines="1"
            android:ellipsize="end"
            android:paddingLeft="@dimen/standard_margin"
            android:paddingTop="10dp"
            android:paddingRight="@dimen/standard_margin"
            android:paddingBottom="8dp"
            app:fontCustom="shp_iranSansMobile" />

    </androidx.cardview.widget.CardView>


    <com.ali.rnp.khodemon.MyLibrary.MyTextView
        android:id="@+id/textInputLayout15234567"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:gravity="center"
        android:text="تلفن تماس"
        android:textSize="14sp"
        app:fontCustom="shp_iranSansMobileBold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fragment_add_level_three_cardView_chooseHours">


    </com.ali.rnp.khodemon.MyLibrary.MyTextView>
    <androidx.cardview.widget.CardView
        android:id="@+id/fragment_add_level_three_cardView_phone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        app:cardCornerRadius="8dp"
        app:cardElevation="2dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textInputLayout15234567">

        <com.ali.rnp.khodemon.MyLibrary.MyEditText
            android:id="@+id/fragment_add_level_three_EditText_phone"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white"
            android:drawableRight="@drawable/ic_maracas"
            android:ellipsize="end"
            android:gravity="center"
            android:hint="0916 *** ****"
            android:inputType="number"
            android:maxLength="11"
            android:maxLines="1"
            android:paddingLeft="@dimen/standard_margin"
            android:paddingTop="12dp"
            android:paddingRight="@dimen/standard_margin"
            android:paddingBottom="10dp"
            android:textSize="14sp"
            app:fontCustom="shp_iranSansMobile"

            />
    </androidx.cardview.widget.CardView>


    <com.ali.rnp.khodemon.MyLibrary.MyTextView
        android:id="@+id/textInputLayout1523474567"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:gravity="center"
        android:text="سال تاسیس"
        android:textSize="14sp"
        app:fontCustom="shp_iranSansMobileBold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fragment_add_level_three_cardView_phone">


    </com.ali.rnp.khodemon.MyLibrary.MyTextView>

    <androidx.cardview.widget.CardView
        android:id="@+id/fragment_add_level_three_cardView_since"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="24dp"
        app:cardCornerRadius="8dp"
        app:cardElevation="2dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textInputLayout1523474567">

        <com.ali.rnp.khodemon.MyLibrary.MyEditText
            android:id="@+id/fragment_add_level_three_EditText_since"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white"
            android:drawableRight="@drawable/ic_maracas"
            android:ellipsize="end"
            android:gravity="center"
            android:hint="1375"
            android:inputType="number"
            android:maxLength="4"
            android:maxLines="1"
            android:paddingLeft="@dimen/standard_margin"
            android:paddingTop="12dp"
            android:paddingRight="@dimen/standard_margin"
            android:paddingBottom="10dp"
            android:textSize="14sp"
            app:fontCustom="shp_iranSansMobile"

            />
    </androidx.cardview.widget.CardView>





</androidx.constraintlayout.widget.ConstraintLayout>

</ScrollView>